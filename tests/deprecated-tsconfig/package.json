{
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "transform": {
      ".(ts|tsx)": "../../preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "coverageReporters": [
      "json"
    ],
    "collectCoverageFrom": [
      "Hello.ts",
      "NullCoverage.js"
    ],
    "mapCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "jest": "^20.0.4"
  }
}
