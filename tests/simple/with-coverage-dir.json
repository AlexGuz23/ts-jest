{
  "transform": {
    ".(ts|tsx)": "../../preprocessor.js"
  },
  "testResultsProcessor": "../../coverageprocessor.js",
  "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js"
  ],
  "coverageDirectory": "coverage-custom",
  "collectCoverageFrom": [
    "Hello.ts",
    "NullCoverage.js"
  ],
  "coverageReporters": [
    "text"
  ]
}
