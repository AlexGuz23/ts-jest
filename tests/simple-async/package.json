{
  "jest": {
    "rootDir": "./",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/dist/index.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "Hello.ts",
      "NullCoverage.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
