{
  "compilerOptions": {
    "target": "es2015",
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "sourceMap": false,
    "inlineSourceMap": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "forceConsistentCasingInFileNames": true,
    "alwaysStrict": true
  },

  "files": [
    "app.ts",
    "Math.ts"
  ]
}
